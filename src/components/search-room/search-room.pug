include ../filter/filter.pug
include ../selection-rooms/selection-rooms.pug

mixin search-room({ filter, selectionRooms })
  main.search-room__content
    .search-room__container.js-search-room
      aside.search-room__filter.js-search-room__filter
        +filter(filter)
        
      section.search-room__selection-rooms
        +selection-rooms(selectionRooms)